{
    "name": "privatejustice/tramite",
    "description": "Tramite para ajudar no desenvolvimento da justi√ßa privada",
    "type": "library",
    "keywords": [
        "ancapistao",
        "private",
        "justice",
        "private-justice",
        "tramite"
    ],
    "license": "ANCAP",
    "homepage": "https://privatejustice.com/packages/tramite",
    "support": {
        "email": "help@privatejustice.com.br",
        "issues": "https://github.com/privatejustice/tramite/issues",
        "source": "https://github.com/privatejustice/tramite",
        "docs": "https://github.com/privatejustice/tramite/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Private Justice",
            "homepage": "https://privatejustice.com.br",
            "email": "contributing@privatejustice.com.br"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Tramite\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
     "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "check-style": "phpcs -p --standard=PSR12 src --extensions=php",
        "fix-style": "phpcbf -p --standard=PSR12 src --extensions=php"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Tramite\\TramiteProvider"
            ],
            "dont-discover": [
                "scaffold-interface"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
